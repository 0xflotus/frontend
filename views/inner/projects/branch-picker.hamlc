%form
  %input{type: "text", data-bind: "value: settings_branch, typeahead: {source: pretty_branch_names}", required: ""}
  %label{placeholder: "Test these settings on this branch", alt: "Enter a branch name"}
  %input{type: "submit", data-bind: "#{@button_binding}", data-loading-text: "Starting...", value: "Try it now!"}

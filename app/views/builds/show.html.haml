- @logs.each do |l|
  .command
    = l.name
    = surround '(', ')' do
      = l.start_time
      = l.end_time

  .rest
    - if l["out"]
      %table
        %thead
          %tr
            %th Timestamp
            %th stdout
            %th stderr
        %tbody
          - l["out"].each do |o|
            %tr
              %td
                = o["time"]
              %td
                %pre
                  - if o["type"] == "out"
                    = o["message"]
              %td
                %pre
                  - if o["type"] == "err"
                    = o["message"]
